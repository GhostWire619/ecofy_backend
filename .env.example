# Environment Variables for Ecofy Backend API
# Copy this file to .env and fill in the actual values

# Security
SECRET_KEY=your_super_secret_key_here

# Database
DATABASE_URL=sqlite:///./ecofy.db
# For production, use something like: postgresql://user:password@localhost/dbname

# External APIs
WEATHER_API_KEY=your_weather_api_key_here
SATELLITE_API_KEY=your_satellite_api_key_here

# ISDA Africa API credentials
ISDA_USERNAME=your_isda_username_here
ISDA_PASSWORD=your_isda_password_here

# Google Gemini AI API
GOOGLE_API_KEY=your_google_api_key_here